{
  "name": "godhana/starter-kit",
  "description": "Godhana Laravel Starter Kit with Home, About, Contact pages and layout",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Benedict Godhana",
      "email": "benedictgodhana7@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Godhana\\StarterKit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Godhana\\StarterKit\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Godhana\\StarterKit\\GodhanaServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan package:discover --ansi"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
